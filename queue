#include <iostream>
#include <queue>
using namespace std;
int main()
{
    queue<int> q;
    for (int i = 1; i <= 5; i++)
    {
        q.push(i * 5);
    }
    cout << "Size = " << q.size() << endl;
    cout << "Front = " << q.front() << endl;
    cout << "The queue : ";
    for (int i = 1; i <= 5; i++)
    {
        cout << q.front() << " ";
        q.pop();
    }
    cout << endl
         << "Size = " << q.size() << endl;
}
